ditches250.D.Fa<- glmer(Use ~ Ditches250 +Develop500 +Soy500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
ditch.w250.D.Fa<- glmer(Use ~ DitchWavg250 +Develop500 +Soy500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
grass.w250.D.Fa<- glmer(Use ~ GrassWavg250 +Develop500 +Fallow500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
grass.h250.D.Fa<- glmer(Use ~ GrassHavg250 +Develop500 +Forest500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
wi250.D<- glmer(Use ~ Wire250scale +Develop500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)

wi250.D.Cot<- glmer(Use ~ Wire250scale +Develop500 +Cotton500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
woody250.D.Cot<- glmer(Use ~ WoodyPerches250 +Develop500 +Cotton500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
nonnat250.D.Cot<- glmer(Use ~ NonnatPerches250 +Develop500 +Cotton500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
ditches250.D.Cot<- glmer(Use ~ Ditches250 +Develop500 +Cotton500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
ditch.w250.D.Cot<- glmer(Use ~ DitchWavg250 +Develop500 +Cotton500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
grass.w250.D.Cot<- glmer(Use ~ GrassWavg250 +Develop500 +Cotton500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
grass.h250.D.Cot<- glmer(Use ~ GrassHavg250 +Develop500 +Cotton500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
wi250.D<- glmer(Use ~ Wire250scale +Develop500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)




AIC250set1<-AIC(wi250.D.C, wi250.D.Cot, wi250.D.R, wi250.D.S, wi250.D.Fa, wi250.D.Fo, wi250.D,
                woody250.D.Fa, nonnat250.D.Fa, ditches250.D.Fa, ditch.w250.D.Fa, grass.w250.D.Fa, grass.h250.D.Fa,
                woody250.D.Cot, nonnat250.D.Cot, ditches250.D.Cot, ditch.w250.D.Cot, grass.w250.D.Cot, grass.h250.D.Cot)




