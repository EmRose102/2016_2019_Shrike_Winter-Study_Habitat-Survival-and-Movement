di.manmade100<- glmer(Use ~ Ditches100 + NonnatPerches100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
ditches100<- glmer(Use ~ Ditches100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
di.ditch.w100<- glmer(Use ~ Ditches100 + DitchWavg100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
di.grass.w100<- glmer(Use ~ Ditches100 + GrassWavg100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
di.grass.h100<- glmer(Use ~ Ditches100 + GrassHavg100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)

