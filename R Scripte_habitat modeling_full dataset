wi250.D.Fa<- glmer(Use ~ Wire250scale +Develop500 +Fallow500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
wi250.D.Fo<- glmer(Use ~ Wire250scale +Develop500 +Forest500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
wi250.D<- glmer(Use ~ Wire250scale +Develop500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)

wi250.D.Fa<- glmer(Use ~ Wire250scale +Develop500 +Corn500 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
