#__________100 Fixed Effects_____________


#100Round1

woody100<- glmer(Use ~ WoodyPerches100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
manmade100<- glmer(Use ~ NonnatPerches100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
ditches100<- glmer(Use ~ Ditches100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
ditch.w100<- glmer(Use ~ DitchWavg100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
grass.w100<- glmer(Use ~ GrassWavg100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)
grass.h100<- glmer(Use ~ GrassHavg100 + Year + (1|PointID) + (1|BirdID), family = binomial, nAGQ =1, data = LOSH16)

AIC250set1<-AIC(null,woody100,manmade100,ditches100,ditch.w100,grass.w100,grass.h100)

deltaAIC250.1<-AIC250set1-(AIC(ditches100))
table250AIC1<-cbind(AIC250set1,deltaAIC250.1)
table250AIC1

#wire100 best predictor

#100Round2

